 }; \n\n        \n        // console.log(user.username)\n        // make github api call \n        // it will return a promise \n         //const response = axios.get(`https://api.github.com/repos/${user.username}/${user.repo}/commits`); \n         //console.log(response); \n    }\n    const url = 'https://api.github.com/repos/saulRM10/my-app/commits/master'\n    const fetchData = async () =>{\n        try{\n          const response = await axios.get(url); \n          console.log(response); \n        }\n        catch(error){\n          console.log(error.response)\n        }\n    }\n\n    useEffect(()=>{\n      fetchData(); \n    }, []); \n\n    return (\n      <div> \n         <form onSubmit={handleSubmit}> \n\n         <label> Username: </label>\n        <input \n          onChange={handleUsername} \n          type=\"text\" \n          placeholder=\"username\" \n          value={username}\n\n          /> \n\n        <label> Repository: </label>\n        <input \n           onChange={handleRepo}\n          type=\"text\" \n          placeholder=\"repository\" \n          value={repo} \n   \n          />\n\n         <button > verify </button>\n\n         </form>\n       </div>\n    )\n};\n\nexport default Verify; \n"},"lineCount":null}},"error":null,"hash":"0f34ebe2139358b616864ac1c4b104da","cacheData":{"env":{}}}